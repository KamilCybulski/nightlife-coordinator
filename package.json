{
  "name": "nightlife-coordinator",
  "version": "1.0.0",
  "description": "Nightlife coordinator app for fcc backend cert",
  "main": "index.js",
  "engines": {
    "node": "8.0.0"
  },
  "scripts": {
    "heroku-postbuild": "npm run build",
    "start": "node server/build/server.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd server && nodemon index.js",
    "dev:client": "webpack-dev-server --config ./webpack.config.dev.client.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "webpack --config webpack.config.prod.client.js",
    "build:server": "webpack --config webpack.config.prod.server.js",
    "test": "jest --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KamilCybulski/nightlife-coordinator.git"
  },
  "author": "kamyl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KamilCybulski/nightlife-coordinator/issues"
  },
  "homepage": "https://github.com/KamilCybulski/nightlife-coordinator#readme",
  "dependencies": {
    "axios": "0.16.2",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-react": "6.24.1",
    "body-parser": "1.18.2",
    "clean-webpack-plugin": "0.1.16",
    "concurrently": "3.5.0",
    "cookie-parser": "1.4.3",
    "css-loader": "0.28.7",
    "express": "4.15.4",
    "express-session": "1.15.6",
    "html-webpack-plugin": "2.30.1",
    "material-ui": "0.19.2",
    "mongodb": "2.2.31",
    "mongoose": "4.11.13",
    "node-sass": "4.5.3",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "passport-local-mongoose": "4.2.1",
    "prop-types": "15.5.10",
    "react": "15.6.1",
    "react-dom": "15.6.1",
    "react-hot-loader": "3.0.0-beta.6",
